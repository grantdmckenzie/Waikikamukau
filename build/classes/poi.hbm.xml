<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

    <class name="edu.ucsb.geog.poi" table="openpoi">

        <id name="id" column="id">
            <generator class="native" />
        </id>
		
        <property name="name">
            <column name="name" length="16" not-null="true" />
        </property>
        <property name="cat">
            <column name="cat" length="16" not-null="true" />
        </property>
        <property name="lat">
            <column name="lat" length="16" not-null="true" />
        </property>
        <property name="lng">
            <column name="lng" length="16" not-null="true" />
        </property>
        <property name="distance">
            <column name="distance" length="16" not-null="true" />
        </property>
        <property name="uri">
            <column name="uri" length="16" not-null="false" />
        </property>
    </class>
    <sql-query name="callNearbyStoredProcedure">
		<return alias="w_nearby" class="edu.ucsb.geog.poi"/>
		<![CDATA[select * from w_nearby(:lng,:lat,:q)]]>
    </sql-query>
</hibernate-mapping>